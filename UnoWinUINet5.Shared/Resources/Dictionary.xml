  <Dictionary Product="Axis My100R Help" Version="1">
    <Languages>
      <Language DisplayName="Български">bg-BG</Language> <!--Болгарский-->
      <Language DisplayName="Ελληνικά">el-GR</Language> <!--Греческий-->
